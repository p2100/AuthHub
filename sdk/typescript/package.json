{
  "name": "@authhub/sdk",
  "version": "0.1.0",
  "description": "AuthHub TypeScript SDK - 统一权限平台SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "authhub",
    "sso",
    "permission",
    "authentication",
    "authorization"
  ],
  "author": "AuthHub Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/AuthHub.git",
    "directory": "sdk/typescript"
  },
  "dependencies": {
    "jose": "^5.1.3",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
